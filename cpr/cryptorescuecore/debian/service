[Unit]
Description=under cryptorescuecore

[Service]
ExecStart=/usr/bin/env nodejs --max_old_space_size=4096 /opt/under/cryptorescuecore/cryptorescuecore/bin/cryptorescuecored
Restart=always
RestartSec=5
TimeoutStopSec=900
User=cryptorescuecore
WorkingDirectory=/opt/under/cryptorescuecore/home

[Install]
WantedBy=multi-user.target
